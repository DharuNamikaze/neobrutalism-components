{
  "name": "neobrutalism-components",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo run dev",
    "dev:docs": "turbo run dev --filter=./apps/docs",
    "dev:reg": "turbo run dev --filter=./apps/util-registry",
    "lint": "turbo run lint",
    "check-types": "turbo check-types",
    "format:write": "prettier --write \"**/*.{ts,tsx,js}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js}\""
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.12",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "turbo": "^2.4.0"
  },
  "packageManager": "pnpm@9.6.0",
  "engines": {
    "node": ">=20"
  }
}
